{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["app"],
      "includePattern": ".+\\.(jsdoc|tsx)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "better-docs/component",  "node_modules/better-docs/typescript"],
    "typescript": {
    "moduleRoot": "test"
},

    "templates": {
      "better-docs": {
        "name": "My React components"
      }
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
  }